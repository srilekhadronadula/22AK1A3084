def count_char(input_string):
    char_counnt={}
    for char in input_string:
        if char in char_count:
            char_count[char]+=1
        else:
            char_count=1
    for char,count in char_count.items():
        print(f"{char}:{count}")
input_string=input("enter a sring")
count_characters(input_string)